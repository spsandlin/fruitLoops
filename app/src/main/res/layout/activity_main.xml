                <Button
                    android:layout_width="41dp"
                    android:layout_height="41dp"
                    android:text="C"
                    android:id="@+id/note21"
                    android:layout_column="8"
                    android:clickable="true"
                    android:background="#ffa365d9"
                    android:layout_margin="3dp"
                    android:backgroundTint="#ff696969"
                    android:backgroundTintMode="multiply" />
            </TableRow>

        </TableLayout>
    </RelativeLayout>

    <Button
        android:layout_width="188dp"
        android:layout_height="43dp"
        android:text="Scale: Cmaj"
        android:id="@+id/button9"
        android:textAllCaps="false"
        android:layout_above="@+id/InnerRelativeLayout"
        android:layout_alignParentStart="true"
        style="@android:style/Widget.Material.Light.Button.Borderless.Colored"
        android:textColor="#fff4fff3"
        android:elegantTextHeight="false"
        android:clickable="true"
        android:textStyle="normal"
        android:textSize="20dp" />

    <Button
        style="@android:style/Widget.Material.Light.Button.Borderless.Colored"
        android:layout_width="188dp"
        android:layout_height="43dp"
        android:text="Sustain"
        android:id="@+id/button10"
        android:textColor="#ff3eb139"
        android:elegantTextHeight="false"
        android:clickable="true"
        android:textStyle="normal"
        android:textSize="20dp"
        android:layout_above="@+id/InnerRelativeLayout"
        android:layout_alignEnd="@+id/InnerRelativeLayout"
        android:layout_alignTop="@+id/button9" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="376dp"
        android:layout_height="44dp"
        android:layout_above="@+id/button9"
        android:layout_alignParentStart="true">

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#00939393"
            android:id="@+id/tableLayout"
            android:baselineAligned="false"
            android:paddingLeft="0dp"
            android:layout_centerHorizontal="true" >


