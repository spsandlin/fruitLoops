                <Button
                    android:layout_width="41dp"
                    android:layout_height="41dp"
                    android:text="C"
                    android:id="@+id/button"
                    android:layout_column="0"
                    android:clickable="true"
                    android:background="#ff41a4d8"
                    android:layout_margin="3dp"
